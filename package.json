{
  "name": "@shren/fftw-js",
  "version": "0.1.5",
  "description": "Javascript port of FFTW via Emscripten",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run build-cjs & npm run build-esm  & npm run build-types",
    "build-cjs": "esbuild src/index.ts --target=es2020 --bundle --sourcemap --outdir=dist --format=cjs",
    "build-esm": "esbuild src/index.ts --target=es2020 --bundle --sourcemap --outdir=dist/esm --format=esm",
    "build-types": "dts-bundle-generator -o dist/index.d.ts src/index.ts --external-imports",
    "postbuild": "node postbuild.js",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fr0stbyter/fftw-js.git"
  },
  "keywords": [
    "FFT",
    "DSP",
    "FFTW",
    "fast-fourier-transform",
    "digital-signal-processing"
  ],
  "author": "j-funk",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/fr0stbyter/fftw-js/issues"
  },
  "homepage": "https://github.com/fr0stbyter/fftw-js#readme",
  "dependencies": {
    "esbuild": "^0.14.2"
  },
  "devDependencies": {
    "@types/emscripten": "^1.39.6",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.12",
    "dts-bundle-generator": "^6.2.0",
    "esbuild-jest": "^0.5.0",
    "jest": "^27.4.4",
    "typescript": "^4.5.3"
  }
}
